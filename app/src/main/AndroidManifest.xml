<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yonnyul.elementosui">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity_Botones"
            android:label="Botones">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.yonnyul.elementosui.MainActivity"/>
        </activity>

        <activity android:name=".Activity_RadioButton"
            android:label="RadioButton">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.yonnyul.elementosui.MainActivity"/>
        </activity>

        <activity android:name=".Activity_Checkbox"
            android:label="Checkbox">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.yonnyul.elementosui.MainActivity"/>
        </activity>

        <activity android:name=".Activity_Spinner"
            android:label="Spinner">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.yonnyul.elementosui.MainActivity"/>
        </activity>

        <activity android:name=".Activity_Lista"
            android:label="Lista">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.yonnyul.elementosui.MainActivity"/>
        </activity>
    </application>

</manifest>